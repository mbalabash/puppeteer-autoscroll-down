{
  "name": "puppeteer-autoscroll-down",
  "version": "0.2.0",
  "description": "Handle infinite scroll on websites with puppeteer",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write -l '**/*.js'",
